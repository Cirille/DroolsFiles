		$gda : GrlDetAmounts(hpePdtType in ( "CCY FORW" , "CCY SWAP" , "GOLD FORW" , "GOLD SWAP" ) && hacAmtCl in ( "03" , "12" ) && icInd == "N" && derivativesCombiCd != null && ( quotedInd == "N" || quotedInd == null ) && posFairValInd == "Y")
	then
        
end





